{
  "name": "varan-boilerplate",
  "private": true,
  "license": "MIT",
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/ersims/varan-boilerplate/issues"
  },
  "homepage": "https://github.com/ersims/varan-boilerplate#README.md",
  "engines": {
    "node": ">= 10.16.3",
    "npm": ">= 6.9.0"
  },
  "browserslist": [
    ">1%",
    "not dead",
    "not ie < 11"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && npm test"
    }
  },
  "lint-staged": {
    "*.{css,less,sass,scss,html,md,graphql,gql,json,ts,tsx,js,jsx,yml,yaml}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "apply:format": "prettier --write \"**/*.{css,less,sass,scss,html,md,graphql,gql,json,ts,tsx,js,jsx,yml,yaml}\"",
    "build": "varan build",
    "build:analyze": "varan build --analyze",
    "check:format": "prettier --list-different \"**/*.{css,less,sass,scss,html,md,graphql,gql,json,ts,tsx,js,jsx,yml,yaml}\"",
    "check:linting": "eslint --cache --cache-location node_modules/.cache/.eslintcache \"*.{ts,js}?(x)\" \"{src,test,types,webpack}/**/*.{ts,js}?(x)\"",
    "check:types": "tsc --noEmit",
    "check": "npm-run-all check:*",
    "clean": "del-cli ./.coverage ./dist",
    "start": "node dist/server/bin/web.js",
    "prestart:watch": "npm run clean",
    "start:watch": "varan watch -- --inspect",
    "pretest": "npm run check",
    "test": "jest --no-cache",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.3.11",
    "@babel/runtime": "^7.16.0",
    "@hapi/boom": "^9.1.1",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@sanity/block-content-to-react": "^2.0.7",
    "@sanity/client": "^2.2.6",
    "@sanity/image-url": "^0.140.22",
    "@testing-library/react": "^11.2.5",
    "@types/tti-polyfill": "^0.2.0",
    "classnames": "^2.2.6",
    "compression": "^1.7.3",
    "connected-react-router": "^6.9.1",
    "core-js": "^3.9.0",
    "cross-fetch": "^3.0.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.0",
    "express-static-gzip": "^2.1.1",
    "focus-visible": "^5.2.0",
    "graphql": "^15.5.0",
    "history": "^4.7.2",
    "immer": "^8.0.1",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.0",
    "qs": "^6.9.6",
    "react": "^17.0.1",
    "react-css-collapse": "^4.1.0",
    "react-dom": "^17.0.1",
    "react-ga": "^3.3.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^1.0.7",
    "react-hot-loader": "^4.13.0",
    "react-icons": "^4.2.0",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-toastify": "^7.0.3",
    "react-transition-group": "^4.4.1",
    "react-waypoint": "^9.0.3",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.2",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.0.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.6.3",
    "save-dev": "0.0.1-security",
    "serialize-javascript": "^5.0.1",
    "source-map-support": "^0.5.19",
    "styled-components": "^5.2.1",
    "tti-polyfill": "^0.2.2",
    "typesafe-actions": "^5.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.0.0",
    "@hot-loader/react-dom": "^17.0.1",
    "@testing-library/jest-dom": "^5.11.9",
    "@types/babel-core": "^6.25.4",
    "@types/classnames": "^2.2.11",
    "@types/compression": "^1.7.0",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/react-css-collapse": "^3.6.0",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-helmet": "^6.1.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-transition-group": "^4.4.1",
    "@types/redux-immutable-state-invariant": "^2.1.1",
    "@types/redux-logger": "^3.0.8",
    "@types/serialize-javascript": "^5.0.0",
    "@types/styled-components": "^5.1.7",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.21.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "del-cli": "^3.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^5.1.1",
    "jest": "^26.6.3",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.5",
    "varan": "^0.17.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,js}?(x)",
      "*.js"
    ],
    "coverageDirectory": ".coverage",
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "projects": [
      {
        "displayName": "client",
        "moduleNameMapper": {
          "^.+\\.css?$": "<rootDir>/test/__mocks__/styleMock.ts",
          "^.+\\.svg?$": "<rootDir>/test/__mocks__/svgMock.ts"
        },
        "testEnvironment": "jsdom",
        "testMatch": [
          "<rootDir>/src/client/**/*.test.{ts,js}?(x)"
        ],
        "setupFilesAfterEnv": [
          "<rootDir>/test/setupTests.ts"
        ],
        "transform": {
          "^.+\\.(j|t)sx?$": "ts-jest",
          "^.+\\.(styl|css|sass|scss|less|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|vtt|wav|mp3|m4a|aac|oga|ico|webmanifest|xml)$": "jest-transform-stub"
        }
      },
      {
        "displayName": "server",
        "moduleNameMapper": {
          "^.+\\.css?$": "<rootDir>/test/__mocks__/styleMock.ts",
          "^.+\\.svg?$": "<rootDir>/test/__mocks__/svgMock.ts"
        },
        "testEnvironment": "node",
        "testMatch": [
          "<rootDir>/src/server"
        ],
        "setupFilesAfterEnv": [
          "<rootDir>/test/setupTests.ts"
        ],
        "transform": {
          "^.+\\.(j|t)sx?$": "ts-jest",
          "^.+\\.(styl|css|sass|scss|less|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|vtt|wav|mp3|m4a|aac|oga|ico|webmanifest|xml)$": "jest-transform-stub"
        }
      }
    ],
    "silent": false,
    "verbose": true
  }
}
